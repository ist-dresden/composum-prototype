<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpl="http://sling.composum.com/platform/1.0"
          xmlns:cpp="http://sling.composum.com/pages/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cpp:Site">
    <jcr:content
            jcr:primaryType="cpp:SiteConfiguration"
            cpl:last-publicMode="inPlace"
            sling:resourceType="composum/pages/stage/edit/site">
        <style
                jcr:primaryType="nt:unstructured"
                category.edit="composum.pages.components.edit"
                category.view="composum.pages.components.view"/>
        <languages
                jcr:primaryType="nt:unstructured">
            <english
                    jcr:primaryType="nt:unstructured"
                    direction=""
                    key="en"
                    label="english"/>
            <german
                    jcr:primaryType="nt:unstructured"
                    direction=""
                    key="de"
                    label="deutsch"/>
        </languages>
    </jcr:content>
    <assets/>
    <home/>
</jcr:root>

<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        jcr:language="de"
        jcr:mixinTypes="[mix:language]"
        jcr:primaryType="sling:Folder"
        sling:basename="tenant-domain-project">
    <widgets jcr:primaryType="sling:Folder"><!-- widget labels and values -->

        <confidentiality jcr:primaryType="sling:MessageEntry"
                sling:key="Confidentiality" sling:message="Vertraulichkeit"/>
        <confidentiality-hint jcr:primaryType="sling:MessageEntry"
                sling:key="choose an asset confidentiality" sling:message="die Vertraulichkeit festlegen"/>
        <secret jcr:primaryType="sling:MessageEntry"
                sling:key="secret" sling:message="geheim"/>
        <confidential jcr:primaryType="sling:MessageEntry"
                sling:key="confidential" sling:message="vertraulich"/>
        <internal jcr:primaryType="sling:MessageEntry"
                sling:key="internal" sling:message="intern"/>
        <public jcr:primaryType="sling:MessageEntry"
                sling:key="public" sling:message="öffentlich"/>

    </widgets>
    <tools jcr:primaryType="sling:Folder"><!-- tools labels -->

        <mycontext-headline jcr:primaryType="sling:MessageEntry"
                sling:key="My new panel - visible if an Element is selected."
                sling:message="Mein neues Panel - sichtbar, wenn ein Element selektiert ist."/>
        <mycontext-selected jcr:primaryType="sling:MessageEntry"
                sling:key="the selected element:" sling:message="das gewählte Element:"/>

    </tools>
</jcr:root>

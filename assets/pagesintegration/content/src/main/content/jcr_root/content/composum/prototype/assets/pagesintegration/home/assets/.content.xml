<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cpp="http://sling.composum.com/pages/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="cpp:Page"
        binontest="{Binary}">
    <jcr:content
            jcr:primaryType="cpp:PageContent"
            sling:resourceType="composum/pages/components/page"
            template="/libs/composum/pages/components/templates/page/test">
        <main
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/components/container/parsys">
            <row
                    jcr:primaryType="cpp:Container"
                    sling:resourceType="composum/pages/components/container/row"
                    columns="-12-">
                <column-0
                        jcr:primaryType="cpp:Container"
                        sling:resourceType="composum/pages/components/container/row/column">
                    <text
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/pages/components/element/text"
                            text="Some simple asset:"/>
                    <asset
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/prototype/assets/pagesintegration/components/simpleasset"
                            assetPath="/content/composum/prototype/assets/pagesintegration/releasedassets/testbild.png"
                            rendition="medium"
                            variation="square"/>
                    <text_1777266254
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/pages/components/element/text"
                            text="Asset ends."/>
                    <simpleasset
                            jcr:primaryType="cpp:Element"
                            sling:resourceType="composum/prototype/assets/pagesintegration/components/simpleasset"
                            assetPath="/content/composum/prototype/assets/pagesintegration/releasedassets/testbild.png"
                            rendition="medium"
                            variation="thumbnail"/>
                </column-0>
            </row>
        </main>
        <top
                jcr:primaryType="cpp:Container"
                sling:resourceType="composum/pages/components/container/parsys">
            <title
                    jcr:primaryType="cpp:Element"
                    sling:resourceType="composum/pages/components/element/title"
                    subtitle="Demonstrates uses of assets"
                    title="Assets integration">
                <image
                        jcr:primaryType="nt:unstructured"
                        imageRef="/content/composum/prototype/assets/pagesintegration/releasedassets/background/gravitational waves.jpg"/>
            </title>
        </top>
        <navigation
                jcr:primaryType="nt:unstructured"
                title="Playground"/>
    </jcr:content>
</jcr:root>

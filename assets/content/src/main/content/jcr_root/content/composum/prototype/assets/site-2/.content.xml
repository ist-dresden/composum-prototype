<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        jcr:primaryType="sling:Folder">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Assets Example 2">
        <assetconfig
                jcr:primaryType="cpa:AssetConfiguration"
                sling:resourceType="composum/assets/manager/config"
                example.image.path="/content/prototype/composum/assets/site-2/images/image-02.png"
                file.png.quality="82.5">
             <normal
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    categories="[default]">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.aspectRatio="1:1"
                        size.width="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.width="320"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size.width="800"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.width="1200"/>
            </normal>
            <portrait
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    size.aspectRatio="3:5">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.aspectRatio="1:1"
                        size.height="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.height="560"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size.height="800"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.height="1200"/>
            </portrait>
            <square
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    size.aspectRatio="1:1">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[icon]"
                        size.width="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[mobile]"
                        size.width="240"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[tablet,default]"
                        size.width="320"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.width="800"/>
            </square>
            <thumbnail
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation">
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.width="32"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size.width="128"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size.width="512"/>
            </thumbnail>
        </assetconfig>
    </jcr:content>
</jcr:root>

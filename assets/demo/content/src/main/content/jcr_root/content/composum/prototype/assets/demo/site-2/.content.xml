<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        jcr:primaryType="sling:Folder">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Assets Example 2"
            jcr:mixinTypes="[mix:versionable,mix:created,mix:lastModified]">
        <assetconfig
                jcr:primaryType="cpa:AssetConfiguration"
                sling:resourceType="composum/assets/config"
                example_image_path="/content/composum/prototype/assets/demo/site-2/images/image-02.png"
                file_png_quality="82.5">
             <normal
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    categories="[default]">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_aspectRatio="1:1"
                        size_width="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_width="320"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size_width="800"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_width="1200"/>
            </normal>
            <portrait
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    size_aspectRatio="3:5">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_aspectRatio="1:1"
                        size_height="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_height="560"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size_height="800"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_height="1200"/>
            </portrait>
            <square
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation"
                    size_aspectRatio="1:1">
                <thumbnail
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[icon]"
                        size_width="128"/>
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[mobile]"
                        size_width="240"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[tablet,default]"
                        size_width="320"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_width="800"/>
            </square>
            <thumbnail
                    jcr:primaryType="cpa:VariationConfig"
                    sling:resourceType="composum/assets/manager/config/variation">
                <small
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_width="32"/>
                <medium
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        categories="[default]"
                        size_width="128"/>
                <large
                        jcr:primaryType="cpa:RenditionConfig"
                        sling:resourceType="composum/assets/manager/config/rendition"
                        size_width="512"/>
            </thumbnail>
        </assetconfig>
    </jcr:content>
</jcr:root>
